generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lake {
  id               String   @id @default(uuid())
  lakeName         String
  location         String
  logoUrl          String
  mainImageUrl     String
  galleryImageUrls String[] @default([])
  phoneNumber      String
  adminName        String
  email            String
  lakeInfo         String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
